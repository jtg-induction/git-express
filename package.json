{
  "name": "git-express",
  "version": "1.0.0",
  "description": "An application based on github api",
  "main": "index.js",
  "scripts": {
    "start-node": "nodemon src/app.js",
    "start": "webpack --watch --mode=development",
    "dev": "concurrently \"npm run start-node\" \"npm run start\"",
    "build": "webpack --mode=production",
    "lint": "eslint .",
    "format": "prettier --write \"**/*.{js,jsx}\""
  },
  "author": "Arjun Bhatt",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
