{
  "name": "git-express",
  "version": "1.0.0",
  "description": "An application based on github api",
  "main": "index.js",
  "scripts": {
    "start-express": "nodemon src/server.js --watch",
    "start": "NODE_ENV=development webpack --watch --mode=development",
    "dev": "concurrently \"npm run start-express\" \"npm run start\"",
    "build:dev": "NODE_ENV=development webpack --mode=development",
    "build:prod": "NODE_ENV=production webpack --mode=production",
    "lint": "eslint ./src ./client",
    "format": "prettier --write \"**/*.{js,jsx}\""
  },
  "author": "Arjun Bhatt",
  "license": "ISC",
  "dependencies": {
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.0",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "mini-css-extract-plugin": "^2.7.6",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
